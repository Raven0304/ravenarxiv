    <script type="text/javascript">
        function detectmob() {
            if (navigator.userAgent.match(/Android/i) ||
                navigator.userAgent.match(/webOS/i) ||
                navigator.userAgent.match(/iPhone/i) ||
                navigator.userAgent.match(/iPad/i) ||
                navigator.userAgent.match(/iPod/i) ||
                navigator.userAgent.match(/BlackBerry/i) ||
                navigator.userAgent.match(/Windows Phone/i)
            ) {
                return true;
            } else {
                return false;
            }
        }

        moment.locale("az");

        $(document).ready(function() {




            $(document).pjax('a[data-pjax]', '#main_content');
            $(document).pjax('a[data-pjax2]', '#secondary_content');
            $(document).on('pjax:start', function() {
                $("#loader").show();

            });

            $(document).on('pjax:end', function() {
                $("#loader").hide();
                // if(detectmob){
                //          $(".open-left").click();
                //     }
            });
            $(document).on('pjax:timeout', function(event) {
                // Prevent default timeout redirection behavior
                event.preventDefault();
            });
            $(document).on('pjax:popstate', function() {
                var pathname = window.location.href;
                $(".bg-pink").removeClass('bg-pink').addClass('bg-primary');
                $(".widget-panel.widget-style-2").each(function() {
                    var hrefString = $($($(this).children("h6")[0]).children("a" [0])).attr("href");
                    if (pathname.includes(hrefString)) {
                        $(this).removeClass('bg-primary');
                        $(this).addClass('bg-pink');
                    }

                });

            });
        });


        function go_to_main_page(url) {
            $.pjax({
                url: url,
                container: "#main_content"
            });
        }

        go_to_main_page("/studies/notice.php");


        go_to_main_page("/studies/index.php?lec_open_idx=59692&lecture_code=1834&sem_code=20242")    </script>



    <script>
      document
        .querySelector(".custom-openSidebar")
        .addEventListener("click", function () {
          document.querySelector(".custom-sidebar").style.left = "0";
          document.querySelector(".custom-overlay").style.width = "100%";
		  $('body').addClass('custom-overflow');
        });

      document
        .getElementById("closeSidebar")
        .addEventListener("click", function () {
          document.querySelector(".custom-sidebar").style.left = "-70%";
          document.querySelector(".custom-overlay").style.width = "0";
		  $('body').removeClass('custom-overflow');
        });

      document
        .querySelector(".custom-overlay")
        .addEventListener("click", function () {
          document.querySelector(".custom-sidebar").style.left = "-70%";
          document.querySelector(".custom-overlay").style.width = "0";
		  $('body').removeClass('custom-overflow');
        });

		$(document).on('pjax:start', function() {
    // Sidebar'ı güncelle
	document.querySelector(".custom-sidebar").style.left = "-70%";
          document.querySelector(".custom-overlay").style.width = "0";
		  $('body').removeClass('custom-overflow');
		});




		
		// Pjax işlemi başladığında
$(document).on('pjax:start', function() {
    // Sidebar'ı güncelle
    document.querySelector(".custom-sidebar").style.left = "-70%";
    document.querySelector(".custom-overlay").style.width = "0";
});

// Pjax işlemi başladığında
$(document).on('pjax:success', function(event) {
    var url = event.target.URL; // veya event.target.location.href
    // Şimdi URL'yi kullanabilirsiniz

    $(document).ready(function() {
        var currentURL = window.location.href;
        //console.log("Şu anki URL: " + currentURL);

        // Sidebar'daki linklerin URL'lerini al ve kontrol et
        $('.custom-sidebar a').each(function() {
            var linkURL = $(this).attr('href');
            var params = new URLSearchParams(linkURL);
            if (params.has('lec_open_idx')) {
                // Eğer user_id parametresi varsa, onun değerini kontrol et
                var userId = params.get('lec_open_idx');
                if (currentURL.indexOf('lec_open_idx=' + userId) !== -1) {
                    $(this).addClass('active'); // Eşleşen link'e 'active' sınıfını ekle
					$('#fenler').addClass('active');
					
					initializeSubmenuToggle()
					
                } else {
                    $(this).removeClass('active'); // Eşleşmeyen link'ten 'active' sınıfını kaldır
                }
            } 
			
			else {
             


            if (currentURL.indexOf(linkURL) !== -1) {
                $(this).addClass('active'); // Eşleşen link'e 'active' sınıfını ekle
            } else {
                $(this).removeClass('active'); // Eşleşmeyen link'ten 'active' sınıfını kaldır
            }
        



            }


        });

    });




	
});
let isSubmenuOpen = false;

function openSubmenu(item) {
  const submenu = item.nextElementSibling;
  item.classList.add('custom-submenu-open');
  submenu.style.maxHeight = submenu.scrollHeight + "px";
  isSubmenuOpen = true;
}

function closeSubmenu(item) {
  const submenu = item.nextElementSibling;
  item.classList.remove('custom-submenu-open');
  submenu.style.maxHeight = null;
  isSubmenuOpen = false;
}

function toggleSubmenu(item) {
  if (item.classList.contains('custom-submenu-open')) {
    closeSubmenu(item);
  } else {
    openSubmenu(item);
  }
}

function initializeSubmenuToggle() {
  document.querySelectorAll('.custom-submenu-toggle.active').forEach(item => {
    openSubmenu(item);
  });

  document.querySelectorAll('.custom-submenu-toggle').forEach(item => {
    item.addEventListener('click', function() {
      toggleSubmenu(this);
    });
  });

  
}

document.addEventListener("DOMContentLoaded", function() {
  initializeSubmenuToggle();
});

document.addEventListener('pjax:start', function() {
  if (isSubmenuOpen) {
    closeSubmenu(document.querySelector('.custom-submenu-toggle.custom-submenu-open'));
  }
});




    </script>

	




function viewMenu(num1, num2, num3, which1, which2) {
	switch(num1) {
		case '0i' :
		break;
		case '1i' :
		break;
		case '2i' :
		break;
		case '3i' :
			switch(num2) {
				case 'nj' :
					if(menu3i0j.style.display=='none') {
						menu3i0j.style.display='';
						document.getElementById(which1).src=openImg1.src;
						document.getElementById(which2).src=openImg2.src;
					}
					else {
						if(document.getElementById('menu3i0j0k') && document.getElementById('menu3i0j0k').style.display == '') viewMenu('3i', '0j', 'nk', 'menu3i0jsign', 'menu3i0jsign2');
						menu3i0j.style.display='none';
						document.getElementById(which1).src=closeImg1.src;
						document.getElementById(which2).src=closeImg2.src;
					}
					break;
				case '0j' :
				break;
			}
		break;
		case '4i' :
		break;
		case '5i' :
		break;
		case '6i' :
			if(menu6i0j.style.display=='none') {
				menu6i0j.style.display='';
				menu6i1j.style.display='';
				menu6i2j.style.display='';
				menu6i3j.style.display='';
				menu6i4j.style.display='';
				document.getElementById(which1).src=openImg1.src;
				document.getElementById(which2).src=openImg2.src;
			}
			else {
				if(document.getElementById('menu6i0j0k') && document.getElementById('menu6i0j0k').style.display == '') viewMenu('6i', '0j', 'nk', 'menu6i0jsign', 'menu6i0jsign2');
				menu6i0j.style.display='none';
				if(document.getElementById('menu6i1j0k') && document.getElementById('menu6i1j0k').style.display == '') viewMenu('6i', '1j', 'nk', 'menu6i1jsign', 'menu6i1jsign2');
				menu6i1j.style.display='none';
				if(document.getElementById('menu6i2j0k') && document.getElementById('menu6i2j0k').style.display == '') viewMenu('6i', '2j', 'nk', 'menu6i2jsign', 'menu6i2jsign2');
				menu6i2j.style.display='none';
				if(document.getElementById('menu6i3j0k') && document.getElementById('menu6i3j0k').style.display == '') viewMenu('6i', '3j', 'nk', 'menu6i3jsign', 'menu6i3jsign2');
				menu6i3j.style.display='none';
				if(document.getElementById('menu6i4j0k') && document.getElementById('menu6i4j0k').style.display == '') viewMenu('6i', '4j', 'nk', 'menu6i4jsign', 'menu6i4jsign2');
				menu6i4j.style.display='none';
				document.getElementById(which1).src=closeImg1.src;
				document.getElementById(which2).src=closeImg2.src;
			}
		break;
	}
}


